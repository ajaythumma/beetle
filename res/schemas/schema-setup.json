{
	"$id": "schema://beetle/schema-setup.json",
	"$schema": "http://json-schema.org/draft-07/schema",
	"type": "object",
	"required": [
		"server"
	],
	"additionalProperties": false,
	"properties": {
		"proxies": {
			"type": "array",
			"items": {
				"$ref": "./schema-configuration.json#/"
			}
		},
		"server": {
			"type": "object",
			"additionalProperties": false,
			"properties": {
				"port": {
					"type": "number"
				},
				"protocol": {
					"$ref": "schema-server-library.json#/definitions/protocol"
				}
			}
		}
	}
}
